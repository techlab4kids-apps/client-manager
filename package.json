{
  "name": "client-manager-techlab4kids",
  "version": "1.0.0",
  "description": "Gestore dei client delle reti TechLAB4Kids",
  "author": "Giancarlo OrrÃ¹ (TechLAB4Kids)",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "start-server": "ts-node server/server.ts",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "dev win": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm:electron\"",
    "build": "react-scripts build",
    "postbuild": "tsc && mv dist/server build && rm -rf dist",
    "postbuildlinux": "tsc && mv dist/server build && rm -rf dist",
    "postbuildwin": "tsc && xcopy dist/server build && rmdir /s dist",
    "electron": "wait-on tcp:3000 && electron-forge start",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make",
    "test": "jest"
  },
  "proxy": "http://localhost:5500/",
  "dependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.6",
    "@reduxjs/toolkit": "^1.8.6",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.3",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.2",
    "immutable": "^4.1.0",
    "lodash": "^4.17.13",
    "morgan": "^1.10.0",
    "pubsub-js": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.1",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@electron-forge/cli": "^6.0.0-beta.66",
    "@electron-forge/maker-deb": "^6.0.0-beta.66",
    "@electron-forge/maker-rpm": "^6.0.0-beta.66",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.66",
    "@electron-forge/maker-zip": "^6.0.0-beta.66",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "browser-sync": "^0.9.1",
    "concurrently": "^5.2.0",
    "dotenv": "^16.0.3",
    "electron": "^20.2.0",
    "foreman": "^2.0.0",
    "jest": "^29.2.2",
    "jest-react-native": "^18.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.8.3",
    "wait-on": "^5.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron-react-express-boilerplate"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
