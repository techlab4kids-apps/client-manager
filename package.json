{
  "name": "client-manager-techlab4kids",
  "version": "23.10.1",
  "description": "Gestore dei client delle reti TechLAB4Kids",
  "author": "Giancarlo OrrÃ¹ (TechLAB4Kids)",
  "main": "public/electron.js",
  "homepage": "./",
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "start": "react-scripts start",
    "start-server": "ts-node server/server.ts",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "dev win": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm:electron\"",
    "build": "react-scripts build",
    "postbuild": "tsc && mv dist/server build && rm -rf dist",
    "postbuildlinux": "tsc && mv dist/server build && rm -rf dist",
    "postbuildwin": "tsc && xcopy dist/server build && rmdir /s dist",
    "electron": "wait-on tcp:3000 && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.17",
    "async-wait-until": "^2.0.12",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.3",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.2",
    "immutable": "^4.1.0",
    "lodash": "^4.17.13",
    "morgan": "^1.10.0",
    "pubsub-js": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.4.1",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3",
    "styled-components": "^5.3.5",
    "typescript": "4.9.3",
    "websocket-client": "^1.0.0",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@electron-forge/cli": "^7.3.0",
    "@electron-forge/maker-deb": "^7.3.0",
    "@electron-forge/maker-rpm": "^7.3.0",
    "@electron-forge/maker-squirrel": "^7.3.0",
    "@electron-forge/maker-zip": "^7.3.0",
    "@types/node": "^18.11.11",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "browser-sync": "^0.9.1",
    "concurrently": "^5.2.0",
    "dotenv": "^16.0.3",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "electron-forge-maker-appimage": "^24.13.3",
    "foreman": "^3.0.1",
    "jest": "^29.2.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "react-scripts": "^5.1.0-next.14",
    "react-test-renderer": "^18.2.0",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "wait-on": "^5.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron-react-express-boilerplate"
          }
        },
        {
          "name": "electron-forge-maker-appimage",
          "platforms": ["linux"],
          "config": {  }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        }
      ]
    }
  }
}
